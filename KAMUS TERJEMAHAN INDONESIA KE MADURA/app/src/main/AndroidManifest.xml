<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.skripsi.cobain">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon01"
        android:label="KAMUS"
        android:roundIcon="@drawable/icon01"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.skripsi.cobain.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.skripsi.cobain.kamus"></activity>
        <activity android:name="com.example.skripsi.cobain.tambah_data"></activity>
        <activity android:name="com.example.skripsi.cobain.tampil_data"></activity>
        <activity android:name="com.example.skripsi.cobain.tentang_plikasi"></activity>
    </application>

</manifest>